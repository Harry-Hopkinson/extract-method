{
  "name": "extract-method",
  "displayName": "extract-method",
  "description": "A vscode extension that gives the functionality to extract a method.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "publisher": "HarryHopkinson",
  "categories": [
    "Formatters",
    "Linters",
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:extract-method.extract-method"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extract-method.extract-method",
        "title": "Extract Method"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "1.64.0",
    "@types/glob": "7.2.0",
    "@types/mocha": "9.1.0",
    "@types/node": "14.18.12",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.9.0",
    "glob": "7.2.0",
    "mocha": "9.2.1",
    "typescript": "4.5.5",
    "ts-loader": "9.2.6",
    "webpack": "5.69.1",
    "webpack-cli": "4.9.2",
    "@vscode/test-electron": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Hopkinson/extract-method"
  },
  "icon": "images/icon.png",
  "author": "Harry Hopkinson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Harry-Hopkinson/extract-method/issues"
  }
}
